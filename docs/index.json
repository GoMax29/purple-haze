  "api_endpoints": {
    "fetchMeteoData": {
      "file": "pages/api/fetchMeteoData.js",
      "description": "Module principal de récupération des données météorologiques",
      "endpoints": [
        {
          "method": "GET",
          "path": "/api/fetchMeteoData",
          "params": ["ville"],
          "description": "Récupère les données météo pour une ville donnée"
        }
      ],
      "functions": [
        {
          "name": "fetchMeteoData",
          "description": "Fonction principale de récupération des données",
          "params": ["ville"],
          "returns": "Object - Données unifiées des 3 APIs"
        },
        {
          "name": "clearCache",
          "description": "Vide le cache manuellement",
          "params": [],
          "returns": "void"
        },
        {
          "name": "getCacheStats",
          "description": "Retourne les statistiques du cache",
          "params": [],
          "returns": "Object - Statistiques détaillées"
        },
        {
          "name": "getSupportedCities",
          "description": "Liste les villes supportées",
          "params": [],
          "returns": "Array<string> - Liste des villes"
        }
      ]
    }
  },
  "external_apis": {
